{
  "family": "task-phpmyadmin-<氏名>-<日付>",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::<ACCOUNT_ID>:role/TaskExecutionRole-<氏名>-<日付>",
  "containerDefinitions": [
    {
      "name": "phpmyadmin",
      "image": "phpmyadmin:5.2.1",
      "memory": 512,
      "cpu": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "protocol": "tcp"
        }
      ],
      "environment": [
        { "name": "PMA_HOST", "value": "sampledb" },
        { "name": "PMA_USER", "value": "root" },
        { "name": "PMA_PASSWORD", "value": "root1234" }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/task-<氏名>-<日付>",
          "awslogs-region": "ap-northeast-1",
          "awslogs-stream-prefix": "phpmyadmin"
        }
      }
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024"
}
